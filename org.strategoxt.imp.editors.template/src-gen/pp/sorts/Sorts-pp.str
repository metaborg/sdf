module src-gen/pp/sorts/Sorts-pp

imports
  libstratego-gpp
  runtime/tmpl/pp
  src-gen/signatures/sorts/Sorts-sig


imports
  src-gen/signatures/kernel/Kernel-sig
  src-gen/signatures/symbols/Symbols-sig


imports
  src-gen/pp/kernel/Kernel-pp
  src-gen/pp/symbols/Symbols-pp


strategies
  prettyprint-Sort =
    ![S(<is-string>)]

  prettyprint-example =
    prettyprint-Sort


strategies
  prettyprint-example =
    prettyprint-Symbol

  prettyprint-example =
    prettyprint-SymbolDef

  prettyprint-example =
    prettyprint-Grammar

  prettyprint-Symbol :
    Sort(t0__) -> [ H(
                      [SOpt(HS(), "0")]
                    , [t0__']
                    )
                  ]
    with t0__' := <pp-one-Z(prettyprint-Sort)> t0__

  is-Symbol =
    ?Sort(_)

  prettyprint-Symbol :
    ParameterizedSort(t0__, parameters__) -> [ H(
                                                 [SOpt(HS(), "0")]
                                               , [ t0__'
                                                 , S(" [[")
                                                 , parameters__'
                                                 , S("]]")
                                                 ]
                                               )
                                             ]
    with t0__' := <pp-one-Z(prettyprint-Sort)> t0__
    with parameters__' := <pp-H-list(prettyprint-Symbol|", ")> parameters__

  is-Symbol =
    ?ParameterizedSort(_, _)

  prettyprint-SymbolDef :
    SortDef(t0__) -> [ H(
                         [SOpt(HS(), "0")]
                       , [t0__']
                       )
                     ]
    with t0__' := <pp-one-Z(prettyprint-Sort)> t0__

  is-SymbolDef =
    ?SortDef(_)

  prettyprint-SymbolDef :
    Cf(t0__) -> [ H(
                    [SOpt(HS(), "0")]
                  , [t0__', S("-CF")]
                  )
                ]
    with t0__' := <pp-one-Z(prettyprint-SymbolDef)> t0__

  is-SymbolDef =
    ?Cf(_)

  prettyprint-SymbolDef :
    Lex(t0__) -> [ H(
                     [SOpt(HS(), "0")]
                   , [t0__', S("-LEX")]
                   )
                 ]
    with t0__' := <pp-one-Z(prettyprint-SymbolDef)> t0__

  is-SymbolDef =
    ?Lex(_)

  prettyprint-SymbolDef :
    Var(t0__) -> [ H(
                     [SOpt(HS(), "0")]
                   , [t0__', S("-VAR")]
                   )
                 ]
    with t0__' := <pp-one-Z(prettyprint-SymbolDef)> t0__

  is-SymbolDef =
    ?Var(_)

  prettyprint-Grammar :
    Sorts(t0__) -> [ H(
                       [SOpt(HS(), "0")]
                     , [S("sorts")]
                     )
                   , H(
                       []
                     , [S("")]
                     )
                   , t0__'
                   , H(
                       []
                     , [S("")]
                     )
                   ]
    with t0__' := <pp-indent(|"2")> [<pp-one-Z(prettyprint-Symbols)> t0__]

  is-Grammar =
    ?Sorts(_)

  is-Symbol =
    fail

  is-SymbolDef =
    fail

  is-Grammar =
    fail