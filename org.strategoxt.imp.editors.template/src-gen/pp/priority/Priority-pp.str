module src-gen/pp/priority/Priority-pp

imports
  libstratego-gpp
  runtime/tmpl/pp
  src-gen/signatures/priority/Priority-sig


imports
  src-gen/signatures/kernel/Kernel-sig
  src-gen/signatures/constants/NatCon-sig
  src-gen/signatures/TemplateLang-sig


imports
  src-gen/pp/kernel/Kernel-pp
  src-gen/pp/constants/NatCon-pp
  src-gen/pp/TemplateLang-pp


strategies
  prettyprint-example =
    prettyprint-Associativity

  prettyprint-example =
    prettyprint-Attribute

  prettyprint-example =
    prettyprint-ArgumentIndicator

  prettyprint-example =
    prettyprint-Group

  prettyprint-example =
    prettyprint-PriorityProduction

  prettyprint-example =
    prettyprint-PriorityProductions

  prettyprint-example =
    prettyprint-Priority

  prettyprint-example =
    prettyprint-Priorities

  prettyprint-example =
    prettyprint-Grammar

  prettyprint-Associativity :
    Left() -> [ H(
                  [SOpt(HS(), "0")]
                , [S("left")]
                )
              ]

  is-Associativity =
    ?Left()

  prettyprint-Associativity :
    Right() -> [ H(
                   [SOpt(HS(), "0")]
                 , [S("right")]
                 )
               ]

  is-Associativity =
    ?Right()

  prettyprint-Associativity :
    NonAssoc() -> [ H(
                      [SOpt(HS(), "0")]
                    , [S("non-assoc")]
                    )
                  ]

  is-Associativity =
    ?NonAssoc()

  prettyprint-Associativity :
    Assoc() -> [ H(
                   [SOpt(HS(), "0")]
                 , [S("assoc")]
                 )
               ]

  is-Associativity =
    ?Assoc()

  prettyprint-Attribute :
    Bracket() -> [ H(
                     [SOpt(HS(), "0")]
                   , [S("bracket")]
                   )
                 ]

  is-Attribute =
    ?Bracket()

  prettyprint-Attribute :
    Assoc(t0__) -> [ H(
                       [SOpt(HS(), "0")]
                     , [t0__']
                     )
                   ]
    with t0__' := <pp-one-Z(prettyprint-Associativity)> t0__

  is-Attribute =
    ?Assoc(_)

  prettyprint-ArgumentIndicator :
    Default(arguments__) -> [ H(
                                [SOpt(HS(), "0")]
                              , [ S("<")
                                , arguments__'
                                , S(">")
                                ]
                              )
                            ]
    with arguments__' := <pp-H-list(prettyprint-NatCon|",")> arguments__

  is-ArgumentIndicator =
    ?Default(_)

  prettyprint-Group :
    WithArguments(t0__, t1__) -> [ H(
                                     [SOpt(HS(), "0")]
                                   , [t0__', S(" "), t1__']
                                   )
                                 ]
    with t0__' := <pp-one-Z(prettyprint-Group)> t0__
    with t1__' := <pp-one-Z(prettyprint-ArgumentIndicator)> t1__

  is-Group =
    ?WithArguments(_, _)

  prettyprint-Group :
    NonTransitive(t0__) -> [ H(
                               [SOpt(HS(), "0")]
                             , [t0__', S(".")]
                             )
                           ]
    with t0__' := <pp-one-Z(prettyprint-Group)> t0__

  is-Group =
    ?NonTransitive(_)

  prettyprint-Group :
    SimpleGroup(t0__) -> [ H(
                             [SOpt(HS(), "0")]
                           , [t0__']
                           )
                         ]
    with t0__' := <pp-one-Z(prettyprint-PriorityProduction)> t0__

  is-Group =
    ?SimpleGroup(_)

  prettyprint-Group :
    ProdsGroup(t0__) -> [ H(
                            [SOpt(HS(), "0")]
                          , [ S("{")
                            , t0__'
                            , S("}")
                            ]
                          )
                        ]
    with t0__' := <pp-one-Z(prettyprint-PriorityProductions)> t0__

  is-Group =
    ?ProdsGroup(_)

  prettyprint-Group :
    AssocGroup(t0__, t1__) -> [ H(
                                  [SOpt(HS(), "0")]
                                , [ S("{")
                                  , t0__'
                                  , S(":")
                                  ]
                                )
                              , t1__'
                              ]
    with t0__' := <pp-one-Z(prettyprint-Associativity)> t0__
    with t1__' := <pp-indent(|"2")> [ <pp-one-Z(prettyprint-PriorityProductions)> t1__
                                    , S("}")
                                    ]

  is-Group =
    ?AssocGroup(_, _)

  prettyprint-PriorityProduction :
    t0__ -> [ H(
                [SOpt(HS(), "0")]
              , [t0__']
              )
            ]
    where not(is-PriorityProduction)
    where t0__' := <pp-one-Z(prettyprint-SdfProduction)> t0__

  prettyprint-PriorityProduction :
    t0__ -> [ H(
                [SOpt(HS(), "0")]
              , [t0__']
              )
            ]
    where not(is-PriorityProduction)
    where t0__' := <pp-one-Z(prettyprint-Production)> t0__

  prettyprint-PriorityProductions :
    list__ -> [ H(
                  [SOpt(HS(), "0")]
                , [list__']
                )
              ]
    where not(is-PriorityProductions)
    where list__' := <pp-V-list(prettyprint-PriorityProduction)> list__

  prettyprint-Priority :
    Chain(list__) -> [ H(
                         [SOpt(HS(), "0")]
                       , [list__']
                       )
                     ]
    with list__' := <pp-H-list(prettyprint-Group|" > ")> list__

  is-Priority =
    ?Chain(_)

  prettyprint-Priority :
    Assoc(t0__, t1__, t2__) -> [ H(
                                   [SOpt(HS(), "0")]
                                 , [ t0__'
                                   , S(" ")
                                   , t1__'
                                   , S(" ")
                                   , t2__'
                                   ]
                                 )
                               ]
    with t0__' := <pp-one-Z(prettyprint-Group)> t0__
    with t1__' := <pp-one-Z(prettyprint-Associativity)> t1__
    with t2__' := <pp-one-Z(prettyprint-Group)> t2__

  is-Priority =
    ?Assoc(_, _, _)

  prettyprint-Priorities :
    list__ -> [ H(
                  [SOpt(HS(), "0")]
                , [list__']
                )
              ]
    where not(is-Priorities)
    where list__' := <pp-V-list(prettyprint-Priority|"0", ",")> list__

  prettyprint-Grammar :
    Priorities(t0__) -> [ H(
                            [SOpt(HS(), "0")]
                          , [S("priorities")]
                          )
                        , H(
                            []
                          , [S("")]
                          )
                        , t0__'
                        , H(
                            []
                          , [S("")]
                          )
                        ]
    with t0__' := <pp-indent(|"2")> [<pp-one-Z(prettyprint-Priorities)> t0__]

  is-Grammar =
    ?Priorities(_)

  is-Associativity =
    fail

  is-Attribute =
    fail

  is-ArgumentIndicator =
    fail

  is-Group =
    fail

  is-PriorityProduction =
    fail

  is-PriorityProductions =
    fail

  is-Priority =
    fail

  is-Priorities =
    fail

  is-Grammar =
    fail